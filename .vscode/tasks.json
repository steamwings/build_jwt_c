{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "Build prereq lib",
      "command": "/usr/bin/clang",
      "args": [
        "-fcolor-diagnostics",
        "-fansi-escape-codes",
        "-I${workspaceFolder}/inc",
        "-I/opt/homebrew/Cellar/libjwt/1.16.0/include",
        "-L/opt/homebrew/Cellar/libjwt/1.16.0/lib",
        "-ljwt",
        "-g",
        "${workspaceFolder}/src/.c",
        "-o",
        "${workspaceFolder}/out/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    },
    {
      "type": "cppbuild",
      "label": "Build main",
      "command": "/usr/bin/clang",
      "args": [
        "-fcolor-diagnostics",
        "-fansi-escape-codes",
        "-I${workspaceFolder}/inc",
        "-I/opt/homebrew/Cellar/libjwt/1.16.0/include",
        "-L/opt/homebrew/Cellar/libjwt/1.16.0/lib",
        "-ljwt",
        "-g",
        "${fileDirname}/*.c",
        "-o",
        "${workspaceFolder}/out/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
